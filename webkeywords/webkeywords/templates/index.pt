<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"><metal:html define-macro="main" tal:define="username h.authenticated_userid(request)">
<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<title>argomenti.in</title>
	<link rel="stylesheet" type="text/css" href="${request.static_url('webkeywords:static/css/all.css')}" media="all" />
	<script type="text/javascript" src="${request.static_url('webkeywords:static/js/jquery.js')}"></script>
	<script type="text/javascript" src="${request.static_url('webkeywords:static/js/main.js')}"></script>
</head>
<body>
	<div class="page">
		<div id="wrapper">			<div class="headline">				<h1 tal:content="username"></h1>				<a href="${request.resource_url(request.root, 'logout')}">Disconnetti</a>				<a tal:condition="h.has_permission('administer', request.root, request)" href="${request.resource_url(request.root, 'admin')}">Admin</a>			</div>			<div id="main">				<div id="content">					<ul class="breadcrumbs">						<li><a href="/">HOME</a></li>						<li tal:condition="get_args.get('parent')"><a tal:attributes="href request.resource_url(request.root, query=get_args)">${get_args.get('parent')}</a></li>						<li tal:condition="category_name"><a href="" tal:content="category_name"></a></li>						<li tal:condition="request.GET.get('keyword')"><a tal:attributes="href request.resource_url(request.root, query=get_args)">${request.GET.get('keyword')}</a></li>					</ul>					<div class="table-container">						<div class="holder">							<div class="frame">								<table>									<tr>										<th class="col1">ARGOMENTI TROVATI</th>										<th class="col2">
											<ul>												<li class="${'active' if not request.view_name else ''}"><a tal:attributes="href request.resource_url(request.root, query=get_args)">DA SCRIVERE</a></li>
												<li class="${'active' if request.view_name == 'scritti' else ''}"><a tal:attributes="href request.resource_url(request.root, 'scritti', query=get_args)">SCRITTI</a></li>
												<li class="${'active' if request.view_name == 'bloccati' else ''}"><a tal:attributes="href request.resource_url(request.root, 'bloccati', query=get_args)">BLOCCATI</a></li>											</ul>
										</th>									</tr>
									<tal:r repeat="item keywords"> 
									<tr tal:attributes="class repeat.item.even + repeat.item.odd">
										<td class="col1">
											<tal:i tal:condition='item.has_child'>
												<img src="${request.static_url('webkeywords:static/images/ico05.png')}" alt="" width="12" height="12" />
												<a href="${request.resource_url(request.root, query=dict(get_args, parent=item.keyword))}" tal:content="item.keyword"></a>
											</tal:i>
											<tal:e tal:condition='not item.has_child'>
												<span tal:content="item.keyword"></span>
											</tal:e>
										</td>
										<td class="col2">											<tal:index condition="request.view_name == ''">
											<img class="muovi_scritti link" keyword="${item.keyword}" src="${request.static_url('webkeywords:static/images/ico01.png')}" alt="" width="22" height="22" />
											<img class="muovi_bloccati link" keyword= "${item.keyword}" src="${request.static_url('webkeywords:static/images/ico02.png')}" alt="" width="23" height="22" />											</tal:index>											<tal:scritti_bloccati condition="request.view_name == 'scritti' or request.view_name == 'bloccati'">											<img class="muovi_back link" keyword= "${item.keyword}" src="${request.static_url('webkeywords:static/images/ico03.png')}" alt="" width="23" height="22" />											</tal:scritti_bloccati>
										</td>
									</tr>
									</tal:r>								</table>							</div>						</div>					</div>					<div class="paging">						<a tal:condition="cur_page != 1" href="${request.resource_url(request.root, query=first_args)}" class="first">&laquo;</a>						<a tal:condition="cur_page != 1" href="${request.resource_url(request.root, query=preview_args)}" class="preview">&lt;</a>						<tal:more condition="more_pages">						<a href="${request.resource_url(request.root, query=last_args)}" class="next">&raquo;</a>						<a href="${request.resource_url(request.root, query=end_args)}" class="last">&gt;</a>						</tal:more>						<ul>
							<li tal:repeat="i list_page_args">
								<a tal:omit-tag="i.get('page') == cur_page" href="${request.resource_url(request.root, query=i)}" tal:content="i.get('page')"></a>
							</li>
						</ul>
					</div>				</div>				<div id="sidebar">					<div class="sidebox">						<div class="holder">							<div class="frame">								<div class="heading">									<h3>FILTRA PER PAROLE CHIAVE</h3>								</div>								<form action="" class="search-form" method="get">									<fieldset>										<div class="text">											<input type="text" name="keyword" />										</div>										<input type="submit" class="submit" value="Ok" />									</fieldset>								</form>							</div>						</div>					</div>					<div class="sidebox">						<div class="holder">							<div class="frame">								<div class="heading">									<h3>FILTRA PER CATEGORIA</h3>								</div>								<div class="menu">									<ul>										<li><a href="${request.resource_url(request.root, 'category', 'ambiente', query=get_args)}">Ambiente</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'auto_e_moto', query=get_args)}">Auto e Moto</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'bellezza_e_benessere', query=get_args)}">Bellezza e Benessere</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'casa_e_decorazione', query=get_args)}">Casa e Decorazione</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'cucina_e_alimentazione', query=get_args)}">Cucina e Alimeniazione</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'cultura', query=get_args)}">Cultura</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'economia_e_finanza', query=get_args)}">Economia e Finanza</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'educazione_e_lavoro', query=get_args)}">Educazione e Lavoro</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'moda_e_tendenza', query=get_args)}">Moda e Tendenza</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'sport', query=get_args)}">Sport</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'svago_e_tempo_libero', query=get_args)}">Svago e Tempo Libero</a></li>										<!--
										<li class="active"><a href="#">Tecnologia</a></li>
										-->
										<li><a href="${request.resource_url(request.root, 'category', 'tecnologia', query=get_args)}">Tecnologia</a></li>										<li><a href="${request.resource_url(request.root, 'category', 'viaggi', query=get_args)}">Viaggi</a></li>									</ul>									<span class="mask">&nbsp;</span>								</div>							</div>						</div>					</div>					<div class="text-box">						<p>Argomenti trovati in totale: <strong tal:content="total"></strong></p>						<!--<p>Ultimo aqgiornamento: <strong>27 Marzo 2011</strong></p>-->					</div>				</div>			</div>		</div>
	</div>
</body></metal:html>
</html>